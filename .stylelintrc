{
	"rules": {
		"block-no-empty": null,
		"color-no-invalid-hex": true,
		"declaration-colon-space-after": "always",
		"indentation": ["tab", {
			"except": ["value"]
		}],
		"max-empty-lines": 2,
		"unit-whitelist": ["em", "rem", "%", "s", "ms", "vh", "vw", "vmin", "px", "deg"],
		"selector-no-id": true
	}
}
