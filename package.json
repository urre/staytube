    "webpack-hot-middleware": "^2.12.0",
    "webpack-merge": "^0.14.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/urre/staytube"
  },
  "scripts": {
    "start:webpack": "webpack-dev-server --hot --inline --content-base ./src --history-api-fallback",
    "build": "NODE_ENV=production webpack -p --progress",
    "start": "concurrently 'npm run start:webpack' 'npm run lint:watch'",
    "lint:watch": "npm run lint -- -w --fix",
    "lint": "esw src",
    "deploy": "./deploy.sh"
  },
  "pre-commit": [
    "test",
    "lint"
  ],
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
