    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "basscss-btn": "^1.1.1",
    "basscss-btn-outline": "^1.1.0",
    "basscss-btn-sizes": "^1.2.0",
    "concurrently": "^2.1.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.0.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.0",
